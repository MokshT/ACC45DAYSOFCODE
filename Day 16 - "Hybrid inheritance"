#include<iostream>
using namespace std;
class A
{
    public:
    A()
    {
    cout << "That is a car." << endl;
    }
};
class B:public A
{
    public:
    B()
    {
        cout << "That is a train." << endl;
    }
};
class C:public A
{
    public:
    C()
    {
        cout << "A cute cat." << endl;
    }
};
class D:public C
{
    public:
    D()
    {
        cout << "Teacup is small and cute." << endl;
    }
};
int main()
{
    B b;
    C c;
    D d;   
    return 0;
}
