#include<iostream>
using namespace std;
int main(){
    int arr[10], i, num, index;
    cout << "Enter 10 Numbers: " << endl;;
    for(i=0; i<10; i++)
    cin >> arr[i];
    cout << "Enter the key: " << endl;;
    cin >> num;
    for(i=0; i<10; i++)
    {
        if(arr[i]==num)
        {
            index = i;
            break;
        }
    }
    cout << "Found at:" << index << endl;;
    cout << endl;
    return 0;
}
